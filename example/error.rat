try {
  raise BaseError;
} except (AssertionError) {
  print("Caught AssertError");
} except (ZeroDivisionError as e) {
  print("Tried to divide by zero: %s" % e);
} except {
  # Catch all types of error: BaseError
  print("I dont care about the error");
  raise; # reraise the current error
} else {
  print("No Error Occurred");
} lastly {
  print("Clean UP");
}
